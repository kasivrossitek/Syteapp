<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/color_dark_orange" />
            <padding android:left="@dimen/map_screen_map_property_padding_outer"
                android:bottom="@dimen/map_screen_map_property_padding_outer"
                android:right="@dimen/map_screen_map_property_padding_outer"
                android:top="@dimen/map_screen_map_property_padding_outer"/>
            <corners android:radius="@dimen/map_screen_map_property_corner_radius"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle">
            <solid android:color="@color/color_orange" />
            <corners android:radius="@dimen/map_screen_map_property_corner_radius"/>
            <padding android:left="@dimen/map_screen_map_property_padding_inner"
                android:bottom="@dimen/map_screen_map_property_padding_inner"
                android:right="@dimen/map_screen_map_property_padding_inner"
                android:top="@dimen/map_screen_map_property_padding_inner"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle">
            <solid android:color="@color/color_white" />
            <corners android:radius="@dimen/map_screen_map_property_corner_radius"/>
            <padding android:left="@dimen/map_screen_map_property_padding_inner"
                android:bottom="@dimen/map_screen_map_property_padding_inner"
                android:right="@dimen/map_screen_map_property_padding_inner"
                android:top="@dimen/map_screen_map_property_padding_inner"/>
        </shape>
    </item>



</layer-list>